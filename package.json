    "svg-url-loader": "6.0.0",
    "ts-keycode-enum": "1.0.6",
    "typeorm": "0.2.29",
    "url-loader": "4.1.1"
  },
  "build": {
    "appId": "com.vilokanlabs.jarvis",
    "mac": {
      "category": "public.app-category.video"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
